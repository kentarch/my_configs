use_xft yes
xftfont snap:size=6
xftalpha 0.5
update_interval 1.0
total_run_times 0
temperature_unit celsius
double_buffer yes
no_buffers yes
text_buffer_size 2048
own_window yes
own_window_transparent yes
own_window_hints undecorated,sticky,below,skip_taskbar,skip_pager
